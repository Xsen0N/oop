<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation" xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <Style x:Key="WindowStyle" TargetType="Window">
        <Setter Property="Background" Value="#FFF080" />
        <Setter Property="Foreground" Value="White" />
    </Style>
    <Style x:Key="ListBoxStyle" TargetType="ListBox">
        <Setter Property="Background" Value="#008080" />
        <Setter Property="Foreground" Value="White" />
    </Style>
    <Style x:Key="TabItemStye" TargetType="TabItem">
        <Setter Property="Background" Value="#00C0FF" />
    </Style>
    <Style x:Key="StackPanelStyle" TargetType="StackPanel">
        <Setter Property="Background" Value="#804000" />
    </Style>
    <Style x:Key="TextBoxStyle" TargetType="TextBox">
        <Setter Property="Background" Value="Gray" />
        <Setter Property="Foreground" Value="White" />
    </Style>
    <Style x:Key="ButtonStyle" TargetType="Button">
        <Setter Property="Background" Value="#008080" />
        <Setter Property="Foreground" Value="White" />
    </Style>
    <Style x:Key="DataGridStyle" TargetType="DataGridCell">
        <Setter Property="Background" Value="#006080" />
        <Setter Property="Foreground" Value="White" />
    </Style>
    <Style x:Key="UniformGridStyle" TargetType="UniformGrid">
        <Setter Property="Background" Value="#FF8000" />
        
    </Style>
</ResourceDictionary>